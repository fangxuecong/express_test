<% include _head %>
<!-- ================================================================ -->
<div id="container">
  <header>
    <h1>
      <a id="logo" href="/" title="Node.js起始页">Node.js</a>
    </h1>
  </header>
  
  <div class="content container clearfix">
    <div class="formWrap">
      <form id="signUp">
        <ul class="form">
          <li>
            <label>用户名</label>
            <input type="text" name="fname">
          </li>
          <li>
            <label>请输入密码</label>
            <input type="password" name="fpassword">
          </li>
          <li>
            <label>请再次输入密码</label>
            <input type="password" name="fpasswordagain">
          </li>
        </ul>
        <a class="submitBtn fBtn2" href="javascript:void(0);">注册<span class="to">&gt;</span></a>
      </form>
    </div>
  </div>
  
  <footer>
    <div class="footer container">
      <% include _footer %>
    </div>
  </footer>
</div>
<!-- ================================================================ -->
<% include _foot %>








